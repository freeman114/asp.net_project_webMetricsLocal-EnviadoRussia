    <script>
        function building() {
            //alert('Em construção!');
            $('#building').modal('show');
        }
        function modalMsg(msg) {
            //alert('Em construção!');
            $('#modalMsgTxt').text(msg);
            $('#modalMsg').appendTo("body").modal('show');
        }
        $(document).ready(function () {
            $('.em-contrucao').on('click', function () {
                building();
            })
        });
    </script>
    <script>
        if ($.isFunction(this.checkLoginState)) {
            window.fbAsyncInit = function () {
                FB.init({
                    appId: '220440968764019',
                    cookie: true,
                    xfbml: true,
                    status: false,
                    version: 'v3.1'
                });
                FB.AppEvents.logPageView();
                FB.Event.subscribe('auth.authResponseChange', function (response) {
                    if (response.status === 'connected' && $.isFunction(this.checkLoginState)) {
                        checkLoginState();
                    } else if (response.status === 'not_authorized') {
                        FB.login();
                    } else {
                        FB.login();
                    }
                });
            };
            (function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) { return; }
                js = d.createElement(s); js.id = id;
                js.src = "https://connect.facebook.net/pt_Br/sdk.js";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        }
    </script>
