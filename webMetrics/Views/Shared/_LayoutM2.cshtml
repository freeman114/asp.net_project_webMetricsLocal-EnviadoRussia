        <div class="container-big">
            <nav class="navbar navbar-expand-lg navbar-light">
                <h1 class="navbar-brand">
                    <a class="logo" href="@Url.Action("Index", "Home")">
                        <img alt="INFLUENCERS INC. Metrics" src="~/content/img/logo.png" />
                    </a>
                </h1>

                <button id="dropdownMenu2" class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarMenu" aria-controls="navbarMenu" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse justify-content-end" id="navbarMenu" aria-labelledby="dropdownMenu2">
                    <div class="language-select order-1 order-lg-3">
                        <!-- este select gera um novo html
                            vc pode dar um id para ele encontra-lo
                            pelo [data-id] em um button que vai ser gerado
                            - os options virarao uma lista dentro da class [dropdown-menu open]
                        -->
                        <select class="selectpicker" data-width="fit">
                            <option data-content='<span class="flag-br"></span> POR'> POR </option>
                            <option data-content='<span class="flag-en"></span> ENG'> ENG </option>
                            <option data-content='<span class="flag-es"></span> SPN'> SPN </option>
                        </select>
                    </div>

                    @{
                        var picture = Microsoft.AspNetCore.Http.SessionExtensions.GetString(Context.Session, "ProfilePicture");
                        var name = Microsoft.AspNetCore.Http.SessionExtensions.GetString(Context.Session, "nomeagencia");
                        var route = Context.Request.Path.Value;
                        if (string.IsNullOrWhiteSpace(picture) == false && route != "/" && route.ToLower() != "/Relatorios/Login".ToLower())
                        {
                            <ul class="navbar-nav order-3 order-lg-2">
                                <li class="nav-item">
                                    <span class="etc">@name</span>
                                </li>
                                <li class="nav-item">
                                    <img class="avatar" src="@picture" alt="icone de perfil" onerror="this.src='../content/img/noimg.jpg'" />
                                </li>
                            </ul>
                        }
                        else
                        {
                            <ul class="navbar-nav order-3 order-lg-2">
                                <li class="nav-item">
                                    <a class="btn btn-gray-outline" href="@Url.Action("Login", "Relatorios")">login</a>
                                </li>
                                <li class="nav-item">
                                    <a class="btn btn-light-blue btn-shadow" href="@Url.Action("ComeceAqui", "Relatorios")">comece aqui</a>
                                </li>
                            </ul>
                        }
                    }
                    @*@if (@ViewBag?.NameUser == null && @ViewBag?.ProfileFotoURI == null)
                        {
                        }
                        else
                        {
                        }*@
                </div>
            </nav>
        </div>
